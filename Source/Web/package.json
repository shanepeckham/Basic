{
  "name": "web",
  "version": "1.0.0",
  "description": "We've made some big updates in this release, so it’s **important** that you spend a few minutes to learn what’s new.",
  "main": "web.js",
  "scripts": {
    "install": "jspm install && bower install && dotnet restore",
    "start": "gulp"
  },
  "jspm": {
    "dependencies": {
      "html": "github:Hypercubed/systemjs-plugin-html@^0.0.8",
      "jquery": "npm:jquery@^3.1.1",
      "knockout": "github:knockout/knockout@^3.4.0",
      "oidc-client": "npm:oidc-client@^1.3.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.15"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "babel-polyfill": "npm:babel-polyfill",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0"
    },
    "overrides": {
      "github:knockout/knockout@3.4.0": {
        "main": "dist/knockout.debug",
        "meta": {
          "dist/knockout.debug.js": {
            "exports": "ko",
            "format": "global"
          }
        }
      },
      "npm:babel-core@5.8.38": {
        "main": "browser.js",
        "dependencies": {},
        "jspmNodeConversion": false,
        "map": {
          "regenerator/runtime": "babel-runtime/regenerator/runtime"
        },
        "meta": {
          "browser.js": {
            "exports": "babel",
            "format": "global"
          }
        }
      },
      "npm:babel-runtime@5.8.38": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:pbkdf2@3.0.12": {
        "main": "browser.js"
      },
      "npm:safe-buffer@5.1.0": {
        "browser": "index.js"
      }
    }
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.6.5",
    "babel-plugin-transform-decorators": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "electron-prebuilt": "^1.3.1",
    "eslint": "^3.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-express": "^0.3.5",
    "gulp-jspm": "^0.5.7",
    "gulp-less": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-requirejs": "^0.1.3",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-start-process": "^1.1.1",
    "gulp-watch": "^4.3.5",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jspm": "^0.17.0-beta.24",
    "jspm-github": "^0.14.9",
    "jspm-npm": "^0.29.2",
    "jspm-registry": "^0.4.1",
    "karma": "^0.13.21",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai-plugins": "^0.7.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "karma-jspm": "^2.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "multimatch": "^2.1.0",
    "phantomjs": "^2.1.3",
    "phantomjs-prebuilt": "^2.1.4",
    "ps-node": "^0.1.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.2"
  },
